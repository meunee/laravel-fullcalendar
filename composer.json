{
    "name": "meunee/laravel-fullcalendar",
    "description": "Laravel 10 helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruslan Murzabaev",
            "email": "ymptnss@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-0": {
            "Meunee\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meunee\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Meunee\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}